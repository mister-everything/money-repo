{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "start": {},
    "lint": {
      "dependsOn": ["^lint"],
      "cache": false
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "test:coverage": {
      "dependsOn": ["^test:coverage"]
    },
    "test:e2e": {
      "dependsOn": ["^test:e2e"]
    },
    "test:ui": {
      "dependsOn": ["^test:ui"]
    },
    "db:migrate": {
      "dependsOn": ["^db:migrate"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
